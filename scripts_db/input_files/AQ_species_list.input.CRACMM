# setenv AEROSOL Variables (1-10)  - compute average over time

species_CASTNET <- "
# setenv GAS Variables (1-10)  - compute average over time
# Model output was originally in ppm, but conversions were already
# made in the combine extract to convert to ug/m3.

  setenv GAS_1 \"nhno3,ug/m3,HNO3_UGM3,,HNO3\"                 # nitric acid
  setenv GAS_2 \"total_so2,ug/m3,SO2_UGM3,,SO2\"               # sulfur dioxide (total SO2   Whatman Filter + 0.667*Nylon Filter)
  setenv GAS_3 \"1.15*total_so2,ug/m3,SO2_UGM3,,SO2_adj\"      # adjusted SO2 value to account for observation bias (experimental)

# setenv AEROSOL Variables  - compute average over time

  setenv AERO_1 \"tso4,ug/m3,ASO4IJ,ug/m3,SO4\"                               # sulfate
  setenv AERO_2 \"tno3,ug/m3,ANO3IJ,ug/m3,NO3\"                       	      # nitrate
  setenv AERO_3 \"tnh4,ug/m3,ANH4IJ,ug/m3,NH4\"                     	      # ammonium
  setenv AERO_4 \"tno3+nhno3,ug/m3,ANO3IJ+HNO3_UGM3,ug/m3,TNO3\"    	      # total nitrate
  setenv AERO_5 \"tso4,ug/m3,ASO4IJ+ASO4K,ug/m3,SO4_IJK\"                     # sulfate
  setenv AERO_6 \"tno3,ug/m3,ANO3IJ+ANO3K,ug/m3,NO3_IJK\"                     # nitrate
  setenv AERO_7 \"tnh4,ug/m3,ANH4IJ+ANH4K,ug/m3,NH4_IJK\"                     # ammonium
  setenv AERO_8 \"tno3+nhno3,ug/m3,ANO3IJ+ANO3K+HNO3_UGM3,ug/m3,TNO3_IJK\"    # total nitrate
  setenv AERO_9 \"tso4,ug/m3,PM25_SO4,ug/m3,PM25_SO4\"                        # sulfate using sharp cutoff
  setenv AERO_10 \"tno3,ug/m3,PM25_NO3,ug/m3,PM25_NO3\"                        # nitrate using sharp cutoff
  setenv AERO_11 \"tnh4,ug/m3,PM25_NH4,ug/m3,PM25_NH4\"                        # ammonium using sharp cutoff
  setenv AERO_12 \"tno3+nhno3,ug/m3,PM25_NO3+HNO3_UGM3,ug/m3,PM25_TNO3\"       # total nitrate using sharp cutoff
  setenv AERO_13 \"MG,ug/m3,AMGJ,ug/m3,MG\"                                    # magnesium
  setenv AERO_14 \"CA,ug/m3,ACAJ,ug/m3,CA\"                                   # calcium
  setenv AERO_15 \"K,ug/m3,AKJ,ug/m3,K\"                                      # potassium
  setenv AERO_16 \"NA,ug/m3,ANAIJ,ug/m3,NA\"                                  # sodium
  setenv AERO_17 \"CL,ug/m3,ACLIJ,ug/m3,CL\"                                  # chloride
  setenv AERO_18 \"MG,ug/m3,AMGJ+AMGK,ug/m3,MG_IJK\"                           # magnesium
  setenv AERO_19 \"CA,ug/m3,ACAJ+ACAK,ug/m3,CA_IJK\"                          # calcium
  setenv AERO_20 \"K,ug/m3,AKJ+AKK,ug/m3,K_IJK\"                              # potassium
  setenv AERO_21 \"NA,ug/m3,ANAIJ+ANAK,ug/m3,NA_IJK\"                         # sodium based on IJK
#  setenv AERO_22 \"CL,ug/m3,ACLIJ+ACLK,ug/m3,CL_IJK\"                         # chloride based on IJK
  setenv AERO_22 \"NA,ug/m3,PMC_NA+PM25_NA,ug/m3,NA_PMC\"                     # sodium based on PMC
  setenv AERO_23 \"CL,ug/m3,PMC_CL+PM25_CL,ug/m3,CL_PMC\"		      # chloride based on PMC
"
  if (ACLK_exists == "T") {
     species_CASTNET <- paste(species_CASTNET, "  setenv AERO_24 \"CL,ug/m3,ACLIJ+ACLK,ug/m3,CL_IJK\"                         # chloride based on IJK
